def sort(num_list):
    N = len(num_list)
    for run in range(N - 1):
        for i in range(N - 1 - run):
            # print(f"сравниваем {num_list[i]} с {num_list[i+1]}")
            if num_list[i] > num_list[i + 1]:
                num_list[i], num_list[i + 1] = num_list[i + 1], num_list[i]
    return num_list


def binary_search(num_list, element):
    min_list = min(num_list)
    max_list = max(num_list)
    middle = (min_list + max_list) // 2
    if num_list[middle-1] == element:
        return middle - 1
    elif num_list[middle] > element:
        return binary_search(num_list, element, min_list, middle - 1)
    else:
        return binary_search(num_list, element, middle + 1, max_list)

